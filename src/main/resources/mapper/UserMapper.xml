<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.thinkmoon.blog.modules.pojo.dao.UserDAO">
    <select id="queryUserByAccount" resultType="cn.thinkmoon.blog.modules.pojo.po.UserPO">
        SELECT
        *
        FROM
        user
        WHERE
        user.`account` = #{account}
    </select>
</mapper>
